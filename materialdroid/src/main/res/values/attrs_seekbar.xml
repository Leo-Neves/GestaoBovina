<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SeekBar">
        <attr name="sb_trackSize" format="reference|dimension"/>
        <attr name="sb_primaryColor" format="reference|color"/>
        <attr name="sb_secondaryColor" format="reference|color"/>
        <attr name="sb_trackCap" format="integer">
            <enum name="butt" value="0x00000000" />
            <enum name="round" value="0x00000001" />
            <enum name="square" value="0x00000002" />
        </attr>
        <attr name="sb_thumbBorderSize" format="reference|dimension"/>
        <attr name="sb_thumbRadius" format="reference|dimension"/>
        <attr name="sb_thumbFocusRadius" format="reference|dimension"/>
        <attr name="sb_travelAnimDuration" format="reference|integer"/>
        <attr name="sb_transformAnimDuration" format="reference|integer"/>
        <attr name="sb_interpolator" format="reference"/>
        <attr name="sb_minValue" format="reference|integer"/>
        <attr name="sb_maxValue" format="reference|integer"/>
        <attr name="sb_stepValue" format="reference|integer"/>
        <attr name="sb_value" format="reference|integer"/>
        <attr name="sb_discreteMode" format="reference|boolean"/>
        <attr name="sb_fontFamily" format="string|reference"/>
        <attr name="sb_textStyle" format="integer">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="sb_seekBarTextSize" format="reference|dimension"/>
        <attr name="sb_seekBarTextColor" format="reference|color"/>
        <attr name="sb_alwaysFillThumb" format="reference|boolean"/>
        <attr name="sb_baselineOffset" format="reference|dimension"/>
        <attr name="sb_titulo" format="string" />
        <attr name="sb_unidade" format="string" />
        <attr name="sb_minimo" format="integer" />
        <attr name="sb_maximo" format="integer" />
        <attr name="sb_discreto" format="boolean" />
        <attr name="sb_tipo" format="enum">
            <enum name="inteiro" value="0"/>
            <enum name="decimal" value="1"/>
        </attr>
        <attr name="sb_cor" format="color" />
        <attr name="android:gravity" />
        <attr name="android:enabled" />
    </declare-styleable>
</resources>